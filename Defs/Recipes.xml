<Defs>

	<RecipeDef>
		<defName>MakeConcrete</defName>
		<label>make 30 concrete</label>
		<description>Mix crushed rock, sand and cement with water to form concrete.</description>
		<jobString>Mixing cement.</jobString>
		<workSkillLearnFactor>0.25</workSkillLearnFactor>
		<effectWorking>CutStone</effectWorking>
		<soundWorking>Recipe_MakeStoneBlocks</soundWorking>
		<workAmount>800</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>CrushedRocks</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>SandPile</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>RFFCement</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<products>
			<RFFConcrete>30</RFFConcrete>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>CrushedRocks</li>
				<li>SandPile</li>
				<li>RFFCement</li>
			</thingDefs>
		</fixedIngredientFilter>
		<workSkill>Crafting</workSkill>
		<recipeUsers>
			<li>TableStonecutter</li>
		</recipeUsers>
	</RecipeDef>

